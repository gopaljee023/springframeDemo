<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
"http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<bean id="triangleid" class="com.jee.Triangle">
	  <property name="pointA" ref="zeroPoint"/>
	   <property name="pointB" ref="point1"/>
	    <property name="pointC" ref="point2"/>
	
	 </bean>
	 
	 
	 <bean id ="zeroPoint" class="com.jee.Point">
	  	<property name="x" value="0"/>
	  	<property name="y" value="0"/>
	 </bean>
	 
	  <bean id ="point1" class="com.jee.Point">
	  	<property name="x" value="-30"/>
	  	<property name="y" value="0"/>
	 </bean>
	 
	  <bean id ="point2" class="com.jee.Point">
	  	<property name="x" value="0"/>
	  	<property name="y" value="30"/>
	 </bean>
	
</beans>